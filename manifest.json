{
  "name": "Ship It",
  "manifest_version": 2,
  "version": "0.1.0",
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./src/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "src/shipit.js"
  ]
}
